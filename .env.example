# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Database Configuration
# Default is sqlite.db in the project root
# DATABASE_URL=sqlite.db

# Application Configuration
# VITE_APP_URL is used for Telegram WebApp buttons and Webhook registration
# Example: https://ngfit-pro.bothost.ru
VITE_APP_URL=https://your-app-url.bothost.ru

# Node Environment
NODE_ENV=production

# Port
PORT=3000

# Admin Configuration
# Set this to your Telegram ID to get admin privileges
# Example: 544475140
OWNER_OPEN_ID=544475140
